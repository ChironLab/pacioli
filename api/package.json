{
  "name": "api",
  "version": "0.0.1",
  "description": "backend",
  "scripts": {
    "test": "jest",
    "postinstall": "yarn generate",
    "build": "tsc",
    "start": "node dist/src/index.js",
    "dev": "NODE_ENV=development ts-node-dev -r dotenv/config ./src/index.ts",
    "generate": "SHOULD_SCHEMA_GEN=true prisma generate && ts-node -T ./src/graphql/index.ts",
    "migrate": "prisma migrate",
    "lint": "eslint src/ --ext .ts",
    "format": "prettier . --write",
    "seed": "ts-node -T ./src/scripts/seed.ts"
  },
  "private": true,
  "devDependencies": {
    "@types/faker": "^5.5.8",
    "@types/jest": "^26.0.23",
    "@types/node": "^16.4.3",
    "@types/pino": "^6.3.11",
    "@typescript-eslint/eslint-plugin": "^4.29.0",
    "dotenv": "8.2.0",
    "eslint": "7.31.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-config-prettier": "^7.2.0",
    "faker": "^5.5.3",
    "jest": "26.6.0",
    "prettier": "^2.3.2",
    "ts-jest": "26.5.6",
    "ts-node": "^9.1.1",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.3.5"
  },
  "dependencies": {
    "@prisma/client": "^2.28.0",
    "apollo-server-fastify": "^3.1.2",
    "date-fns": "^2.23.0",
    "fastify": "^3.19.2",
    "fastify-cors": "^6.0.2",
    "fastify-helmet": "^5.3.2",
    "fastify-static": "^4.2.3",
    "graphql": "^15.5.1",
    "graphql-middleware": "^6.0.10",
    "graphql-scalars": "^1.10.0",
    "nexus": "^1.1.0",
    "pino": "^6.13.0",
    "pino-pretty": "^5.1.2",
    "prisma": "^2.28.0"
  }
}
